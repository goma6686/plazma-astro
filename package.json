{
  "name": "solar-singularity",
  "version": "4.2.0",
  "type": "module",
  "scripts": {
    "dev": "npx tinacms dev -c \"astro dev",
    "start": "tinacms build && astro dev",
    "build": "tinacms build && astro check && astro build && jampack ./dist",
    "preview": "astro preview",
    "sync": "astro sync",
    "astro": "astro",
    "format:check": "prettier --check . --plugin=prettier-plugin-astro",
    "format": "prettier --write . --plugin=prettier-plugin-astro",
    "cz": "cz",
    "prepare": "husky install && npm install --cpu=x64 --os=linux --libc=musl sharp",
    "lint": "eslint ."
  },
  "dependencies": {
    "@astrojs/check": "^0.5.6",
    "@astrojs/mdx": "^2.2.2",
    "@astrojs/rss": "^4.0.2",
    "@resvg/resvg-js": "^2.6.0",
    "@sentry/astro": "^7.108.0",
    "@spotlightjs/astro": "^1.0.18",
    "@types/react-dom": "^18.2.22",
    "astro": "^4.2.1",
    "astro-purgecss": "^4.1.0",
    "astro-tunnel": "^0.1.4",
    "build": "^0.1.4",
    "fuse.js": "^7.0.0",
    "github-slugger": "^2.0.0",
    "install": "^0.13.0",
    "moment": "^2.30.1",
    "npm": "^10.8.2",
    "prosemirror-utils": "^1.2.0",
    "react-datetime": "^3.2.0",
    "react-markdown": "^9.0.1",
    "react-tinacms-editor": "^0.53.26",
    "remark-collapse": "^0.1.2",
    "remark-toc": "^9.0.0",
    "run": "^1.5.0",
    "satori": "^0.10.11",
    "styled-components": "^6.0.0",
    "stylis": "^4.0.0",
    "tailwindcss": "^3.4.1",
    "tinacms": "^1.6.0",
    "tiptap": "^1.32.2",
    "tiptap-extensions": "^1.35.2",
    "typescript": "^5.4.2",
    "vite": "^5.2.6"
  },
  "devDependencies": {
    "@astrojs/react": "^3.1.0",
    "@astrojs/sitemap": "^3.0.5",
    "@astrojs/tailwind": "^5.1.0",
    "@divriots/jampack": "^0.23.2",
    "@tailwindcss/typography": "^0.5.10",
    "@tinacms/cli": "^1.5.42",
    "@types/github-slugger": "^1.3.0",
    "@types/node": "^20.14.11",
    "@types/react": "^18.2.48",
    "@typescript-eslint/parser": "^6.19.0",
    "astro-eslint-parser": "^0.16.2",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.56.0",
    "eslint-plugin-astro": "^0.31.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.4",
    "prettier-plugin-astro": "^0.13.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.33.4"
  },
  "overrides": {
    "prosemirror-tables": "1.2.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,mdx,json,astro}": [
      "prettier --write --plugin=prettier-plugin-astro"
    ]
  }
