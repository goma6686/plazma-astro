{
  "name": "solar-singularity",
  "version": "4.2.0",
  "type": "module",
  "scripts": {
    "dev": "tinacms dev -c \"astro dev\"",
    "tina-dev": "tinacms dev -c \"astro dev\"",
    "start": "tinacms build && astro dev",
    "build": "tinacms build && astro build && jampack ./dist",
    "preview": "astro preview",
    "sync": "astro sync",
    "astro": "astro",
    "format:check": "prettier --check . --plugin=prettier-plugin-astro",
    "format": "prettier --write . --plugin=prettier-plugin-astro",
    "cz": "cz",
    "prepare": "npm install --cpu=x64 --os=linux --libc=musl sharp",
    "local": "tinacms build --local -c \"astro build\""
  },
  "dependencies": {
    "@astrojs/check": "^0.9.5",
    "@astrojs/mdx": "^4.3.7",
    "@astrojs/netlify": "^6.5.13",
    "@astrojs/rss": "^4.0.12",
    "@headlessui/react": "^2.1.2",
    "@resvg/resvg-js": "^2.6.2",
    "@sentry/astro": "^8.19.0",
    "@spotlightjs/astro": "^2.0.0",
    "@tinacms/auth": "^1.0.12",
    "@tinacms/graphql": "^1.5.17",
    "@types/react-dom": "^18.3.0",
    "add-to-calendar-button-react": "^2.8.9",
    "astro": "^5.14.8",
    "astro-embed": "^0.9.0",
    "backend": "^0.0.0",
    "build": "^0.1.4",
    "fuse.js": "^7.0.0",
    "github-slugger": "^2.0.0",
    "graphiql": "^3.3.2",
    "i": "^0.3.7",
    "init": "^0.1.2",
    "lightningcss": "^1.29.3",
    "lite-youtube-embed": "^0.3.2",
    "npm": "^10.9.2",
    "remark-collapse": "^0.1.2",
    "remark-toc": "^9.0.0",
    "run": "^1.5.0",
    "satori": "^0.10.14",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.6",
    "tinacms": "^2.7.6"
  },
  "devDependencies": {
    "@astrojs/react": "^4.4.0",
    "@astrojs/sitemap": "^3.6.0",
    "@astrojs/tailwind": "^6.0.2",
    "@divriots/jampack": "^0.24.4",
    "@tailwindcss/typography": "^0.5.13",
    "@tinacms/cli": "^1.9.6",
    "@types/github-slugger": "^1.3.0",
    "@types/node": "^22.15.17",
    "@types/react": "^18.3.3",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.1.1",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup": "^4.18.1",
    "typescript": "^5.8.3"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.6.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,mdx,json,astro}": [
      "prettier --write --plugin=prettier-plugin-astro"
    ]
  },
  "overrides": {
    "graphql": "15.8.0"
  }
}
